<?xml version="1.0" encoding="UTF-8"?>
<queries>
    <findOneBYid>
        <![CDATA[
        select *
        from usuarios
        where usuario_id = :id

    ]]>
    </findOneBYid>
    <findByEmail>
        <![CDATA[
        select *
        from usuarios
        where email = :email
    ]]>
    </findByEmail>


</queries>
